{
  "name": "phpstorm-theme",
  "displayName": "PhpStorm Dark Theme",
  "description": "Dark Theme - PhpStorm Edition",
  "icon": "./images/icon.png",
  "publisher": "ericgomez",
  "galleryBanner": {
    "color": "#1e1f22",
    "theme": "dark"
  },
  "version": "1.0.17",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark theme",
    "jetbrains",
    "new dark",
    "phpstorm",
    "php"
  ],
  "contributes": {
    "configurationDefaults": {
      "editor.bracketPairColorization.enabled": false
    },
    "themes": [
      {
        "label": "PhpStorm Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-theme.json"
      },
      {
        "label": "PhpStorm Dark Islands",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-theme-islands.json"
      }
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericgomez/vscode-phpstorm-theme.git"
  },
  "bugs": {
    "url": "https://github.com/ericgomez/vscode-phpstorm-theme/issues",
    "email": "eric.gomez.sanchez@gmail.com"
  }
}
